    "# Input: A list of names\n",
    "# Output: Player IDs for each name\n",
    "def get_player_ids(names):\n",
    "    player_ids = []\n",
    "    \n",
    "    for name in names:\n",
    "        print(name)\n",
    "        player_ids.append(players.find_players_by_full_name(name)[0]['id'])\n",
    "        \n",
    "    assert len(names) == len(player_ids), \\\n",
    "    \"Someone is missing\"\n",
    "    return player_ids"
   ]
